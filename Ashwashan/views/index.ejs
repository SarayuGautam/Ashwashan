<div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="max-height: 60rem;">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="/images/bg.jpeg" alt="First slide">
            <div class=" carousel-caption d-none d-md-block" style="margin-bottom: 550px; text-align: right;">
                <h1>Ashwashan</h1>
            </div>
        </div>
        <div class="carousel-item">
