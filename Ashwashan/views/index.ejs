<%- include('partials/upperHeader') %>
<link rel="stylesheet" href="/stylesheets/index.css">

<%- include('partials/lowerHeader')%>

<div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="max-height: 600px;">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="images/bg.jpeg" alt="First slide">
            <div class="carousel-caption d-none d-md-block" style="margin-bottom: 450px;">
                <h5>This is the first slide</h5>
            </div>
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="images/bg.jpeg" alt="Second slide">
            <div class="carousel-caption d-none d-md-block" style="margin-bottom: 450px;">
                <h5>This is second slide</h5>
            </div>
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="images/bg.jpeg" alt="Third slide">
            <div class="carousel-caption d-none d-md-block" style="margin-bottom: 450px;">
                <h5>This is third slide</h5>
            </div>
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<%- include('partials/footer') %>